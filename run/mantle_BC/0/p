FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    top
    {
        type            calculated;
        value           $internalField;
    }

    bottom
    {
        type            calculated;
        value           $internalField;
    }

    left_cyc
    {
        type		cyclic;
    }

    right_cyc
    {
        type		cyclic;
    }

    frontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //
