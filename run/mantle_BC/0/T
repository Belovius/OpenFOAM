FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 0;

boundaryField
{
    top
    {
        type            fixedValue;
        value           uniform 0;
    }
    bottom
    {
        type            fixedValue;
        value           uniform 43.8;//36.5;
    }
    left_cyc
    {
        type		cyclic;
    }

    right_cyc
    {
        type		cyclic;
    }

    frontAndBack
    {
        type            empty;
    }

